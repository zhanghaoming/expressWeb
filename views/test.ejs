<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html manifest="demo.appcache">
<head>
    <title>Music For Web</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <meta name="format-detection" content="telephone=no"/>
    <script type="text/javascript" language="javascript" src="http://libs.baidu.com/jquery/2.0.0/jquery.js"></script>
    <script type="text/javascript" language="javascript">
        function test()
        {
            var info = $("#Minput").val();
            var data = {"info":info};
            alert(info);
            $.ajax({
                type: "POST",
                url: "/login",
                data: data,
                error: function (data,status) {
                    alert(data);
                },
                success: function (data,status) {
                    alert("err");
                }
            });
        }
    </script>
</head>
<body>
	<div>
		<table>
		<% for(var i=0; i<rankList.length; i++) {%>
			<tr>
				<td>
					<input type="text"value="<%= rankList[i].name %>" id="<%= rankList[i].id %>" onclick="songDetial(this)">
				</td>
			</tr>
		<% } %>
		</table>
	</div>
    <input type="text" id="Minput">
    <input type="button" id="button1" onclick="test()" value="submit">
</body>
</html>